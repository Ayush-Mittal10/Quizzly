<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<svg width="1200" height="630" viewBox="0 0 1200 630" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with gradient -->
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#FFAB91" stop-opacity="0.8"/>
      <stop offset="100%" stop-color="#B39DDB" stop-opacity="0.9"/>
    </linearGradient>
  </defs>
  <rect width="1200" height="630" fill="url(#bg-gradient)"/>
  
  <!-- Quiz interface mockup -->
  <rect x="650" y="130" width="450" height="370" rx="15" fill="#FFFFFF" stroke="#4DB6AC" stroke-width="4"/>
  
  <!-- Quiz header -->
  <rect x="650" y="130" width="450" height="60" rx="15" fill="#4DB6AC"/>
  <text x="690" y="170" font-family="Poppins, sans-serif" font-size="24" fill="#FFFFFF" font-weight="bold">Quiz Time</text>
  <circle cx="1050" cy="160" r="20" fill="#FFAB91"/>
  <text x="1050" cy="168" font-family="Poppins, sans-serif" font-size="18" fill="#FFFFFF" text-anchor="middle">10:00</text>
  
  <!-- Quiz content -->
  <rect x="670" y="210" width="410" height="70" rx="5" fill="#F5F5F5"/>
  <text x="690" y="240" font-family="Poppins, sans-serif" font-size="16" fill="#1A1A3D" font-weight="bold">Question 2 of 10</text>
  <text x="690" y="265" font-family="Inter, sans-serif" font-size="15" fill="#1A1A3D">What is the capital of France?</text>
  
  <!-- Options -->
  <rect x="670" y="290" width="410" height="40" rx="20" fill="#FFFFFF" stroke="#4DB6AC" stroke-width="2"/>
  <text x="700" y="315" font-family="Inter, sans-serif" font-size="15" fill="#1A1A3D">A) London</text>
  
  <rect x="670" y="340" width="410" height="40" rx="20" fill="#4DB6AC" stroke="#4DB6AC" stroke-width="2"/>
  <text x="700" y="365" font-family="Inter, sans-serif" font-size="15" fill="#FFFFFF">B) Paris</text>
  <circle cx="1040" cy="360" r="10" fill="#FFFFFF"/>
  <circle cx="1040" cy="360" r="6" fill="#4DB6AC"/>
  
  <rect x="670" y="390" width="410" height="40" rx="20" fill="#FFFFFF" stroke="#B39DDB" stroke-width="2"/>
  <text x="700" y="415" font-family="Inter, sans-serif" font-size="15" fill="#1A1A3D">C) Rome</text>
  
  <!-- Decorative elements -->
  <circle cx="180" cy="150" r="30" fill="#4DB6AC" opacity="0.6"/>
  <circle cx="1000" cy="550" r="25" fill="#FFAB91" opacity="0.6"/>
  <circle cx="250" cy="500" r="35" fill="#B39DDB" opacity="0.6"/>
  <circle cx="1100" cy="200" r="15" fill="#FFB83D" opacity="0.6"/>
  
  <!-- Logo and Text -->
  <g transform="translate(150, 250)">
    <g fill="#FFFFFF">
      <path d="M80,70 a40,40 0 1,1 -80,0 a40,40 0 1,1 80,0 Z" stroke="#FFFFFF" stroke-width="8" fill="none"/>
      <path d="M60,70 a20,20 0 1,1 -40,0 a20,20 0 1,1 40,0 Z" stroke="#FFFFFF" stroke-width="5" fill="none"/>
      <path d="M40,50 L40,90" stroke="#FFFFFF" stroke-width="8" stroke-linecap="round"/>
      <path d="M20,70 L60,70" stroke="#FFFFFF" stroke-width="8" stroke-linecap="round"/>
    </g>
  </g>
  
  <!-- Text -->
  <text x="230" y="250" font-family="Poppins, sans-serif" font-size="90" font-weight="bold" fill="#FFFFFF">Quizzly</text>
  <text x="230" y="320" font-family="Inter, sans-serif" font-size="34" fill="#FFFFFF">Elevate Your Quizzes</text>
  
  <!-- Features -->
  <g fill="#FFFFFF" opacity="0.9">
    <circle cx="250" cy="370" r="8"/>
    <text x="270" y="378" font-family="Inter, sans-serif" font-size="24">Custom interactive quizzes</text>
    
    <circle cx="250" cy="420" r="8"/>
    <text x="270" y="428" font-family="Inter, sans-serif" font-size="24">Stay focused with real-time monitoring</text>
    
    <circle cx="250" cy="470" r="8"/>
    <text x="270" y="478" font-family="Inter, sans-serif" font-size="24">Video connect for better learning</text>
  </g>
  
  <!-- Signature -->
  <text x="600" y="560" font-family="Caveat, cursive" font-size="28" fill="#FFFFFF" text-anchor="middle">Let's ace it!</text>
</svg> 